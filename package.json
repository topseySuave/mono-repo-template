{
  "name": "resitour360",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run --workspace @resitour/web dev",
    "build": "npm run --workspace @resitour/web build",
    "start": "npm run --workspace @resitour/web start",
    "lint": "npm run --workspace @resitour/web lint",
    "dev:server": "npm run --workspace @resitour/server dev",
    "build:server": "npm run --workspace @resitour/server build",
    "start:server": "npm run --workspace @resitour/server start",
    "test:server": "npm run --workspace @resitour/server test",
    "test:server:watch": "npm run --workspace @resitour/server test:watch"
  }
}
